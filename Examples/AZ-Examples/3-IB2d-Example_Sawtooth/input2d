FV_LT_muscle = 0        % 0 (for no) or 1 (for yes) % Force-Velocity & Length-Tension Model
3_element_muscle = 0    % 0 (for no) or 1 (for yes) % Hill 3-Element Model w/ FV-LT Model
arb_ext_force = 0       % 0 (for no) or 1 (for yes)
