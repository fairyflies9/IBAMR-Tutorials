%
% FLUID PARAMETER VALUES -> to get nu=0.01 to match water %
%
Fluid_Parameters {
mu = 2.234            % Dynamic Viscosity (N*s/m^2) 
rho = 1000             % Density (kg/m^2)
}



%
% TEMPORAL INFORMATION %
%
Temporal_Information {
Tfinal = 10.0           % Final time of simulation (s)
dt = 2.5e-5            % Time-step (s)
}


%
% GRID PARAMETERS %
%
Grid_Parameters {
