y_gravity_vec_comp = -1 % y-Component of Gravity Vector
porous_media = 0        % 0 (for no) or 1 (for yes)
poroelastic = 0         % 0 (for no) or 1 (for yes)
electro_phys = 0        % 0 (for no) or 1 (for yes)
